  "keywords": [
    "pokemon"
  ],
  "license": "MIT",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sain-Biswas/pokeapi-to-sql-data-fetcher.git"
  },
  "author": {
    "name": "Sain-Biswas",
    "url": "https://github.com/Sain-Biswas"
  },
